javascript: delay=prompt("Enter delay! Remember! Anything Below 60 will bug out the end screen."); /*Educational purposes*/ var rawElements = document.getElementsByClassName("token_unit");var listElements = Array.from(rawElements); var letterList = []; listElements.forEach(item => letterList.push(item.innerHTML.replace(/<\/?[^>]+(>|$)/g, ""))); var strippedLetterList = letterList.map(function(x){return x.trim().replace(/&nbsp;/g, " ").replace(/&amp/g,"&").replace(/&lt;/g,"<").replace(/&gt;/g,">").replace(/&#09;/g," ").replace(/&#13;/g," ");}); for (let i = 0; i < strippedLetterList.length; i++) {setTimeout(function(){window.core.record_keydown_time(strippedLetterList[i])}, delay*i)};
